.box {
  background-color: #F1F3F4;
  display: flex;
  flex-direction: column;
  height: 100%;
}
.nodata {
  display: grid;
  place-content: center;
  height: 100%;
}
.nodata .contents {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.nodata .contents image {
  width: 150px;
  height: 114px;
}
.nodata .contents .text {
  font-size: 20rpx;
}
.popur {
  width: 552rpx;
  height: 264rpx;
  background: #FFFFFF;
  border-radius: 16rpx 16rpx 16rpx 16rpx;
  padding: 48rpx;
  box-sizing: border-box;
}
.popur .delete {
  font-size: 28rpx;
  color: #14171A;
  text-align: center;
}
.popur .cance-sbmer {
  display: flex;
