.uni-margin-wrap {
  width: 100%;
  height: 750rpx;
  position: relative;
  z-index: 1;
}
.uni-margin-wrap .pic_count {
  position: absolute;
  right: 32rpx;
  bottom: 32rpx;
  background: rgba(0, 0, 0, 0.49);
  border-radius: 22rpx;
  padding: 4rpx 16rpx;
  font-size: 20rpx;
  font-weight: 400;
  color: #FFFFFF;
}
.uni-margin-wrap .swiper {
  height: 100%;
}
.uni-margin-wrap .swiper-img {
  display: block;
  width: 100%;
  height: 750rpx;
  text-align: center;
}
.uni-margin-wrap .swiper-img .swiper-item {
  height: 750rpx;
}
.uni-margin-wrap .swiper-img .swiper-item image {
  width: 100%;
  height: 100%;
}
.uni-margin-wrap  .uni-swiper-dot {
  width: 10rpx;
  height: 10rpx;
  background: rgba(207, 207, 207, 0.69);
}
.uni-margin-wrap  .uni-swiper-dot-active {
  background-color: #fff !important;
}
.uni-margin-wrap .nav_bar {
  position: absolute;
  left: 32rpx;
  right: 0%;
}
.conter {
  position: relative;
  background: #FFF;
  margin-top: -25rpx;
  box-shadow: 0rpx -8rpx 20rpx 0rpx rgba(255, 255, 255, 0.2);
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  z-index: 5 !important;
  padding: 30rpx 32rpx 136rpx;
  box-sizing: border-box;
}
.conter .money {
  display: flex;
  align-items: center;
  margin-top: 16rpx;
}
.conter .money .two {
  font-size: 44rpx;
  color: #FA6B05;
  font-weight: 700;
  display: flex;
  justify-content: center;
  align-items: center;
}
.conter .money .two .moneyIcon {
  font-size: 32rpx;
}
.conter .money .two .moneyMonth {
  font-size: 28rpx;
  color: #616366;
  font-weight: 400;
}
.conter .money .three {
  margin-left: 16rpx;
  font-size: 24rpx;
  color: #B1B2B2;
  text-decoration: line-through;
}
.conter .textwater {
  font-weight: 600;
  font-size: 32rpx;
  color: #2E3033;
  margin-top: 16rpx;
}
.conter .texttext {
  font-size: 28rpx;
  color: #2E3033;
  margin-top: 16rpx;
}
.conter .img-text {
  margin-top: 16rpx;
  position: relative;
}
.conter .img-text image {
  width: 244rpx;
  height: 64rpx;
}
.conter .img-text .texts {
  position: absolute;
  top: 18rpx;
  left: 64rpx;
  font-size: 20rpx;
  color: #FAEBE1;
}
.conter .title {
  margin-top: 48rpx;
  font-weight: 600;
  font-size: 32rpx;
  color: #14171A;
}
.conter .detaimage {
  width: 100%;
  margin-top: 32rpx;
}
.conter .detaimage ._img {
  width: 100%;
}
.bottoms {
  width: 100%;
  height: 136rpx;
  background-color: #FFF;
  position: fixed;
  bottom: 0%;
  left: 0%;
  z-index: 9;
  display: flex;
  padding: 24rpx 30rpx;
  box-sizing: border-box;
}
.bottoms .mall-image {
  text-align: center;
}
.bottoms .mall-image image {
  width: 48rpx;
  height: 48rpx;
  display: block;
}
.bottoms .mall-image .box {
  width: 48rpx;
  height: 48rpx;
  position: relative;
  margin: auto;
}
.bottoms .mall-image .mall {
  margin-top: 8rpx;
  font-size: 24rpx;
  color: #2E3033;
}
.bottoms .gxc {
  margin-left: 40rpx;
}
.bottoms .purchase {
  width: 464rpx;
  height: 72rpx;
  margin-left: 50rpx;
  display: flex;
  border: 2rpx solid #FAA805;
  border-radius: 50rpx;
  line-height: 72rpx;
  text-align: center;
}
.bottoms .purchase .gwc {
  width: 232rpx;
  font-size: 28rpx;
  color: #FA6B05;
}
.bottoms .purchase .gm {
  font-size: 28rpx;
  color: #FFFFFF;
  width: 232rpx;
  background: linear-gradient(144deg, #FAA805 0%, #FA6B05 100%);
  border-radius: 0rpx 100rpx 100rpx 0rpx;
}
.aquaticProduct {
  height: 952rpx;
  background: #FFF;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  padding: 30rpx 32rpx;
  box-sizing: border-box;
}
.aquaticProduct .image-money-icon {
  display: flex;
}
.aquaticProduct .image-money-icon .image image {
  width: 168rpx;
  height: 168rpx;
  border-radius: 12rpx 12rpx 12rpx 12rpx;
}
.aquaticProduct .image-money-icon .money {
  color: #FA6B05;
  margin-left: 16rpx;
}
.aquaticProduct .image-money-icon .money .money-icon {
  font-size: 28rpx;
}
.aquaticProduct .image-money-icon .money .moneyMonth {
  font-size: 36rpx;
}
.aquaticProduct .image-money-icon .money .moneyText {
  font-size: 24rpx;
  color: #616366;
}
.aquaticProduct .image-money-icon .text {
  font-size: 24rpx;
  color: #616366;
  margin-top: 16rpx;
}
.aquaticProduct .image-money-icon .icon {
  margin-left: auto;
}
.aquaticProduct .specifications {
  margin-top: 64rpx;
}
.aquaticProduct .specifications .text {
  font-size: 28rpx;
  color: #14171A;
}
.aquaticProduct .specifications .list {
  display: flex;
  flex-wrap: wrap;
  grid-gap: 16rpx;
  margin-top: 24rpx;
}
.aquaticProduct .specifications .list .text {
  width: 106rpx;
  height: 66rpx;
  background: #FAEBE1;
  border-radius: 8rpx 8rpx 8rpx 8rpx;
  border: 2rpx solid #FA6B05;
  line-height: 66rpx;
  text-align: center;
  color: #FA6B05;
  font-size: 24rpx;
}
.aquaticProduct .specifications .list .texts {
  width: 106rpx;
  height: 66rpx;
  background: #F4F5F6;
  border-radius: 8rpx 8rpx 8rpx 8rpx;
  border: 2rpx solid #F4F5F6;
  line-height: 66rpx;
  text-align: center;
  color: #2E3033;
  font-size: 24rpx;
}
.aquaticProduct .text-number-box {
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 66rpx;
}
.aquaticProduct .text-number-box .text {
  font-size: 28rpx;
  color: #14171A;
}
.aquaticProduct .orderplacement {
  width: 690rpx;
  height: 88rpx;
  background: linear-gradient(144deg, #FAA805 0%, #FA6B05 100%);
  border-radius: 100rpx 100rpx 100rpx 100rpx;
  font-size: 32rpx;
  color: #FFFFFF;
  line-height: 88rpx;
  text-align: center;
  color: #FFF;
  position: absolute;
  bottom: 92rpx;
}

