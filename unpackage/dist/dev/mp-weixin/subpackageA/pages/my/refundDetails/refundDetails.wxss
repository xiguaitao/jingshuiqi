.refundDetails {
  display: flex;
  flex-direction: column;
}
.refundDetails .popur {
  width: 552rpx;
  background: #FFFFFF;
  border-radius: 16rpx 16rpx 16rpx 16rpx;
  padding: 48rpx;
  box-sizing: border-box;
}
.refundDetails .popur .delete {
  font-size: 28rpx;
  color: #14171A;
  text-align: center;
}
.refundDetails .popur .cance-sbmer {
  display: flex;
  justify-content: center;
  margin-top: 64rpx;
}
.refundDetails .popur .cance-sbmer .cance {
  width: 216rpx;
  height: 64rpx;
  border-radius: 100rpx 100rpx 100rpx 100rpx;
  font-size: 28rpx;
  color: #14171A;
  line-height: 64rpx;
  text-align: center;
  border: 2rpx solid #B5B6B6;
}
.refundDetails .popur .cance-sbmer .sbmer {
  background: #2590FA;
  border: none;
  margin-left: 24rpx;
  color: #FFF;
}
.refundDetails .refundDetails-contnet {
  flex: 1;
  overflow: auto;
  padding: 32rpx;
  box-sizing: border-box;
}
.refundDetails .refundDetails-contnet .main {
  background: #FFFFFF;
  border-radius: 24rpx;
